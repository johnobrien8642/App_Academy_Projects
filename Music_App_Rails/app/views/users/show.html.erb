<h1>Current User</h1>

<%= @user.email %>

<form action="<%= session_url %>" method="POST">
    <input type="hidden"
           name="authenticity_token" 
           value="<% form_authenticity_token %>">
  <input type="hidden" name="_method" value="DELETE">
  <input type="submit" value="Log Out">
</form>